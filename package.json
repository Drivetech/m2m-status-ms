{
  "name": "m2m-status-ms",
  "version": "0.0.1",
  "description": "Microservice for check status of your m2mdataglobal sims",
  "main": "src",
  "scripts": {
    "start": "node src",
    "start:dev": "nodemon -- --inspect src"
  },
  "engines": {
    "node": ">=6.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lgaticaq/m2m-status-ms.git"
  },
  "keywords": [
    "sim",
    "m2m",
    "m2mdataglobal"
  ],
  "author": "Leonardo Gatica <lgatica@protonmail.com> (https://about.me/lgaticaq)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lgaticaq/m2m-status-ms/issues"
  },
  "homepage": "https://github.com/lgaticaq/m2m-status-ms#readme",
  "dependencies": {
    "dotenv": "^5.0.0",
    "express": "^4.14.0",
    "express-winston": "^2.1.0",
    "m2m-status": "^1.0.0",
    "raven": "^2.0.0",
    "winston": "^3.3.0",
    "winston-loggly": "^1.3.1"
  },
  "devDependencies": {
    "eslint": "4.19.1",
    "nodemon": "1.19.4"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true,
      "mocha": true
    },
    "extends": "eslint:recommended",
    "rules": {
      "indent": [
        2,
        2
      ],
      "linebreak-style": [
        2,
        "unix"
      ],
      "quotes": [
        2,
        "single"
      ],
      "semi": [
        2,
        "always"
      ]
    }
  }
}
